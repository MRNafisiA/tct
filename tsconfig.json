{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "rootDir": "./src",
    "outDir": "./build-ts",
    "removeComments": true,
    "jsx": "react"
  },
  "include": [
    "./src",
    "./src/custom.d.ts"
  ],
  "exclude": [
    "./build-ts/",
    "./build-webpack-dev/",
    "./build-webpack-prd/",
    "./node_modules/"
  ]
}